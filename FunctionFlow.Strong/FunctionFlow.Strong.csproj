<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Prototypist.FunctionFlow.Strong</RootNamespace>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Prototypist</Authors>
    <Company>Prototypist</Company>
    <Description>"Strongly typed" extensions for function graph</Description>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>Prototypist.pfx</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.1</Version>
    <AssemblyName>FunctionFlow.Strong</AssemblyName>
    <RepositoryType>git</RepositoryType>
    <PackageIconUrl>https://raw.githubusercontent.com/Prototypist1/FunctionGraph/master/icon.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/Prototypist1/FunctionFlow</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Prototypist1/FunctionFlow</RepositoryUrl>
    <Copyright>2018 Colin Wielga</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\FunctionFlow\FunctionFlow.csproj" />
  </ItemGroup>

</Project>
